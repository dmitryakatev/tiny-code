{
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "no-console": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "no-case-declarations": "off",
    "no-plusplus": "off",
    "import/prefer-default-export": "off",
    "no-param-reassign": 0
  },
  "env": {
    "browser": true,
    "node": true
  }
}